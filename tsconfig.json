{
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "node",
    "jsx": "react",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "strict": true,
    "skipLibCheck": true,
    "lib": ["es2015", "dom"]
  },
  "include": [
    "src" // Убедитесь, что ваша папка с исходным кодом включена
  ],
  "exclude": [
    "node_modules" // Исключите node_modules
  ]
}
